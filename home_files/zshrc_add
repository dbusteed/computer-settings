# aliases 
alias python="python3.8"
alias pip="python -m pip"
alias ipy="ipython"
alias py="ipython"
alias jn="jupyter notebook"
alias q="exit"
alias gl="git log --oneline"
alias open="xdg-open"
alias o="xdg-open"
alias dk="docker"
alias g="grep -v grep | grep"
alias R="R --no-save"
alias c="code ."
alias pping="ping -c 3 google.com"